        <item>beginning</item>
        <item>cancel</item>
        <item>forget it</item>
        <item>never mind</item>
        <item>abort</item>
      </one-of>
    </action>
    
    <action name="up">
      <one-of>
        <item>up</item>
        <item>back</item>
        <item>go back</item>
      </one-of>
    </action>

